//O(n) time and O(1) space
class Solution {
    public boolean canJump(int[] nums) {
        if(nums==null || nums.length<2) return true;
        int n = nums.length, maxReach = 0;
        for(int i=0;i<n-1;i++) {
            if(i>maxReach) return false;
            maxReach = Math.max(maxReach, i+nums[i]);
        }
        return (maxReach>=(n-1));
    }
}
