//O(n*sqrt(n)) time and O(n) space
class Solution {
    public int minSteps(int n) {
        int[] dp = new int[n + 1];
        for (int i = 2; i <= n; i++) {
            dp[i] = i; 
            for (int j = 2; j * j <= i; j++) {
                if ((i % j) == 0) {
                    dp[i] = j + dp[i / j];
                    break;
                }
            }
        }
        return dp[n];
    }
}
