//O(n) time and O(1) space
class Solution {
    public void duplicateZeros(int[] arr) {
        int zero = 0;
        for(int a:arr) {
            if(a==0)
                zero++;
        }
        int i = arr.length-1, j = arr.length+zero-1;
        while(i<j) {
            if(arr[i]==0) {
                if(j<arr.length) arr[j] = arr[i];
                j--;
                if(j<arr.length) arr[j] = arr[i];
            }
            else {
                if(j<arr.length) arr[j] = arr[i];
            }
            i--;
            j--;
        }
    }
}
