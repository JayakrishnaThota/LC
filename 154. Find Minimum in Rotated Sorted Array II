//O(logn) time and O(1) space
class Solution {
    public int findMin(int[] nums) {
        if(nums==null || nums.length==0) return 0;
        int n = nums.length;
        int low = 0, high = n-1;
        while(low<high) {
            int mid = low + (high-low)/2;
            if(nums[mid]>nums[high]) low = mid+1;
            else if(nums[mid]<nums[high]) high = mid;
            else high--;
        }
        return nums[low];
    }
}
